        @if (ViewBag.errorMessages != null)
        {
            <div class="container">
                <div class="row alert alert-danger alert-dismissable fade in">
                    <h4>Please correct the following errors and try again</h4>
                    <ul>
                        @foreach (string errorMessage in ViewBag.errorMessages)
                    {
                            <li>
                                @errorMessage
                            </li>
                    }
                    </ul>
                </div>
            </div>
        }
        

    @if (ViewBag.errorMessage != null)
    {
        <div class="container">
            <div class="row alert alert-danger alert-dismissable fade in">
                <h4>Whoops!  Please correct the following error and try again</h4>
                <p>@ViewBag.errorMessage</p>
            </div>
        </div>
    }
    
    @if (TempData["errorMessage"] != null)
    {
        <div class="container">
            <div class="row alert alert-success alert-dismissable fade in">
                <h4>Whoops!  Please correct the following error and try again</h4>
                <p>@TempData["errorMessage"]</p>
            </div>
        </div>
    }

    
    @if (TempData["successMessage"] != null)
    {
        <div class="container">
            <div class="row alert alert-success alert-dismissable fade in">
                <h4>Success</h4>
                <p>@TempData["successMessage"]</p>
            </div>
        </div>
    }






